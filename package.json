{
  "name": "wspr-web",
  "version": "1.0.0",
  "description": "Web Secure P2P Relay - A secure communication platform",
  "scripts": {
    "start": "node server/src/index.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/src/index.js",
    "client:dev": "cd client && npm start",
    "test": "jest",
    "lint": "eslint .",
    "build": "cd client && npm run build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "pg": "^8.11.3",
    "sequelize": "^6.35.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "libsignal": "^2.0.1",
    "node-webrtc": "^0.4.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "eslint": "^8.54.0",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "private": true,
  "author": "",
  "license": "MIT"
}
